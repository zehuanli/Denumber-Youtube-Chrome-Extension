{
  "manifest_version": 2,
  "description": "Disable number key shortcuts for YouTube Video Players, works in both youtube.com and embedded iframes",
  "version": "0.0.1",
  "name": "Denumber YouTube",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon32.png",
      "128": "icons/icon128.png"        
    }
  }
}
